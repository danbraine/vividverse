{
  "version": 1,
  "canisters": {
    "internet_identity": {
      "type": "custom",
      "candid": "https://github.com/dfinity/internet-identity/releases/download/release-2025-11-07-v2/internet_identity.did",
      "wasm": ".dfx/local/canisters/internet_identity/internet_identity_dev.wasm",
      "remote": {
        "id": {
          "ic": "rdmx6-jaaaa-aaaaa-aaadq-cai"
        }
      },
      "frontend": {}
    },
    "coverce_backend": {
      "main": "src/coverce_backend/main.mo",
      "type": "motoko"
    },
    "coverce_frontend": {
      "dependencies": ["coverce_backend"],
      "frontend": {
        "entrypoint": "src/coverce_frontend/src/index.html"
      },
      "source": ["src/coverce_frontend/src"],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    },
    "ic": {
      "type": "persistent"
    }
  },
  "output_env_file": ".env"
}



